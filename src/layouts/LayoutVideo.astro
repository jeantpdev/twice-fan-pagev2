---
import { ViewTransitions } from 'astro:transitions';

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Pagina del Dance Cover Group LILACDREAMERS. Grupo de baile enfocado en el KPOP ubicados en Barranquilla - Colombia." />
		<meta name="keywords" content="Kpop, Dance group, Dance, group, KPOP COVER, KPOP Dance Cover, KPOP Dance Cover Colombia, KPOP Dance Cover Colombia Barranquilla">
		<meta name="author" content="Jean Carlos Trujillo Perez">
		<meta name="date" content="2024-04-19">
		<meta name="robots" content="index, follow">
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<ViewTransitions />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>

